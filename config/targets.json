{
  "GET /api/products": {
    "entry": "frontend",
    "any_of": ["product-catalog"]
  },
  "GET /api/recommendations": {
    "entry": "frontend",
    "any_of": ["recommendation"]
  },
  "GET /api/cart": {
    "entry": "frontend",
